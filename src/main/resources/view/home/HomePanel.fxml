<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Timer">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <ListView fx:id="timerListView" minHeight="138.0" prefHeight="138.0" prefWidth="734.0" />
      <Label text="Last Set">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <ListView fx:id="lastSetView" minHeight="130.0" prefHeight="130.0" prefWidth="734.0" />
      <Label text="Upcoming Sets">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
       <ListView fx:id="ongoingSessionView" prefHeight="400.0" prefWidth="524.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
